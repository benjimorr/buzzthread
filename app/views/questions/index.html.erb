<h1>All Questions</h1>
<% @questions.each do |question| %>
    <div class="media">
        <div class="media-body">
            <h4 class="media-heading">
                <%= link_to question.title, question %>
            </h4>
            <p>Resolved?  <%= question.resolved == true ? "YES" : "NO" %></p>
        </div>
    </div>
<% end %>
